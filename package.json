{
  "name": "@blacksail/e2e-agents",
  "version": "1.0.0",
  "description": "智能浏览器自动化测试代理系统 - 基于 LangChain + Playwright",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "e2e-agents": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "cli": "tsx src/cli.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "tsc --noEmit",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "e2e",
    "testing",
    "playwright",
    "langchain",
    "ai",
    "automation",
    "browser-automation",
    "test-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@langchain/anthropic": "^1.3.7",
    "@langchain/core": "^1.1.12",
    "@langchain/openai": "^1.2.1",
    "commander": "^14.0.2",
    "deep-diff": "^1.0.2",
    "dotenv": "^17.2.3",
    "playwright": "^1.57.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
